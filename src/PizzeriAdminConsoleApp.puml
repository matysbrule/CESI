@startuml
class Pizza {
    - code: String
    - nom: String
    - prix: double

    + Pizza(code: String, nom: String, prix: double)
    + getCode(): String
    + setCode(code: String): void
    + getNom(): String
    + setNom(nom: String): void
    + getPrix(): double
    + setPrix(prix: double): void
    + toString(): String
}

class PizzeriaAdminConsoleApp {
    - afficherPizzas(pizzas: Pizza[]): void
    + main(args: String[]): void
}






